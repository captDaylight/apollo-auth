{
    "extends": "airbnb",
    "rules": {
      "react/destructuring-assignment": [0],
      "react/jsx-filename-extension": [
        1,
        { "extensions": [".js", ".jsx"] }
      ],
      "react/jsx-one-expression-per-line": [0]
    },
    "env": {
        "browser": true,
        "node": true
    }
}
